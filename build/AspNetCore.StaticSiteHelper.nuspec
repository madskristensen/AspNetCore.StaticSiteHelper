<?xml version="1.0" encoding="utf-8"?>
<package>
    <metadata>
        <id>AspNetCore.StaticSiteHelper</id>
        <title>ASP.NET Core Static Site Helper</title>
        <summary>A package that keeps your static website free of boilerplate ASP.NET Core files.</summary>
        <version>1.0.0</version>
        <description>A package that keeps your static website free of boilerplate ASP.NET Core files.</description>
        <authors>Mads Kristensen</authors>
        <projectUrl>https://github.com/madskristensen/AspNetCoreStaticSiteHelper</projectUrl>
        <tags>asp.net core static</tags>
        <iconUrl>https://raw.githubusercontent.com/madskristensen/AspNetCore.StaticSiteHelper/master/icon.png</iconUrl>

        <dependencies>
            <dependency id="Microsoft.AspNetCore" version="1.1.1" />
            <dependency id="Microsoft.AspNetCore.Server.IISIntegration" version="1.1.1" />
            <dependency id="Microsoft.AspNetCore.Server.Kestrel" version="1.1.1" />
            <dependency id="Microsoft.AspNetCore.StaticFiles" version="1.1.1" />
        </dependencies>
    </metadata>

    <files>
        <file src="..\src\*.cs" target="tools" />
        <file src="..\src\*.props" target="build" />
    </files>
</package>